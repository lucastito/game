public class ObjectiveCard extends Card {
	Planet[] planetsToAttack;
	Planet[] planetsToDefend;
	short planetsToChoose;

	public ObjectiveCard(Planet[] planetsToAttack, Planet[] planetsToDefend) {
		this.planetsToAttack = planetsToAttack;
		this.planetsToDefend = planetsToDefend;
		this.planetsToChoose = planetsToChoose;
	}
}